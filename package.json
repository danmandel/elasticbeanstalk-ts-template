{
  "name": "elasticbeanstalk-ts-template",
  "version": "1.0.0",
  "description": "A TypeScript template for an AWS Elastic Beanstalk application",
  "main": "index.js",
  "scripts": {
    "build": "sh ./bin/build.sh",
    "dist": "sh ./bin/dist.sh",
    "deploy": "npm run build && npm run dist && eb deploy --staged",
    "start": "node build/index.js"
  },
  "keywords": [
    "AWS",
    "Elastic Beanstalk",
    "TypeScript",
    "Express"
  ],
  "author": "Dan Mandel <danmandel09@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "typescript": "^4.4.4"
  }
}
